package third_dimention;

import javafx.scene.paint.Color;
import javafx.scene.shape.Box;
import javafx.scene.paint.PhongMaterial;

public class Plane extends Box {
    
    public Plane(double width, double height, double depth) {
        super(width, height, depth); // 비행기의 크기를 설정합니다.
        setMaterial(new PhongMaterial(Color.BLUE)); // 비행기의 색상을 파란색으로 설정합니다.
    }
}
